package GUI;

import javax.swing.*;

import Test.*;

public class Test {
	
	private Exercise model;
	private JPanel view;
	
	public Test(Exercise e, JPanel panel) {
		model = e;
		view = panel;
		
		JPanel unit = new JPanel();
		unit.setLayout(new BoxLayout(unit, BoxLayout.PAGE_AXIS));
		JLabel text = new JLabel(e.showQuestionTest());
		unit.add(text);
		
		view.setVisible(false);
		unit.setVisible(true);
		
		
	}

}
