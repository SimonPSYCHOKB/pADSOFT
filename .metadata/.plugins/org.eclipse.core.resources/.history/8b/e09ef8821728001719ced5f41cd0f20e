package GUI;

import java.awt.event.*;

import Test.*;

import Application.*;

public class OptionsController implements ActionListener {

	private Exercise model;
	private Test view;
	
	public OptionsController(Exercise model, Test view) {
		this.model = model;
		this.view = view;
	}

	@Override
	public void actionPerformed(ActionEvent arg0) {
		UIQuestion question = view.getCenter();
		Answer a = model.answerQuestionTest();
	}

}
