import java.io.BufferedReader;
import java.io.FileReader;
import java.io.IOException;
import java.util.ArrayList;
import java.util.List;
import java.util.StringTokenizer;


public class Tst {
	public static void main(String[] args) throws IOException{
		int i = 0;
		List<Student> students = new ArrayList<Student>();
		String [] data = new String[5];
		FileReader f= new FileReader("src/data.txt");
		BufferedReader buf = new BufferedReader(f);
		String chain = buf.readLine();
		while(chain != null){
			StringTokenizer st = new StringTokenizer(chain, ";");
			while(st.hasMoreTokens() && i<5){
				data[i] = st.nextToken();
				i++;
			}
			Student s = new Student(data[3], data[4], data[0], data[1], data[2]);
			if (students.add(s) == false){
				buf.close();
				f.close();
				
			}
		}
		buf.close();
		f.close();
		System.out.println(students);
		
	}
}
