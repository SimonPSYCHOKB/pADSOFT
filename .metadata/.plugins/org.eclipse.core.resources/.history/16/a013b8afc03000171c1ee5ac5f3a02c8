package gui.windows;

import java.awt.event.ActionEvent;
import java.awt.event.ActionListener;

public class CreateQuestion2 implements ActionListener {
	
	
	private CreateQuestion view;

	public CreateQuestion2(CreateQuestion view){
		this.view = view;
	}

	@Override
	public void actionPerformed(ActionEvent arg0) {
		String wording = view.getWording().getText();
		double penalty = (double) view.getPenalty().getValue();
		double weight = (double) view.getWeight().getValue();
		String type = (String) view.getTypeQuestion().getSelectedItem();
	}

}
