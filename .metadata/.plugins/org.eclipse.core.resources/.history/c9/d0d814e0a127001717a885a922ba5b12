package GUI;

import javax.swing.*;

import Test.*;

public class Test {
	
	private Exercise model;
	private JPanel view;
	
	public Test(Exercise e, JPanel panel) {
		model = e;
		view = panel;
		
		JPanel unit = new JPanel(new BoxLayout(0,1));
		JLabel text = new JLabel(e.showQuestionTest());
		add(text);
		
		view.setVisible(false);
		setVisible(true);
		
		
	}

}
