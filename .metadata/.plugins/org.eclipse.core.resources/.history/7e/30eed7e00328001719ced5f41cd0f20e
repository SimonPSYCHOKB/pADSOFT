package GUI;

import javax.swing.*;

import Test.*;

public class Test extends JFrame{
	
	private static final long serialVersionUID = 1L;
	
	public Test(Exercise e, JPanel panel) {
		model = e;
		view = panel;
		
		JPanel exercise = new JPanel();
		exercise.setLayout(new BoxLayout(exercise, BoxLayout.PAGE_AXIS));
		
		for(Question q : e.getQuestions()){
			JLabel wording = new JLabel(q.getQuestion());
			exercise.add(wording);
		}
		
		exercise.setVisible(true);
	}

}
